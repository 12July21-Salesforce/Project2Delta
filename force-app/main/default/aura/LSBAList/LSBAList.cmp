<aura:component>
    <aura:attribute name="lots" type="Lot__c[]"/>
    <lightning:card title="Lots">
        <p class="slds-p-horizontal_small">
            <aura:iteration items="{!v.lots}" var="lot">
                <c:LSBAItem lot="{!lot}"/>
            </aura:iteration>
        </p>
    </lightning:card>
</aura:component>	
